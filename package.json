{
  "name": "off-import",
  "version": "1.0.5",
  "description": "service d'import OFF",
  "main": "src/index.js",
  "repository": "https://github.com/BlueForestTrees/api-import.git",
  "author": "slim <smedini@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "build": "rimraf dist && babel src --out-dir dist/js",
    "dev": "node --version && cross-env NODE_ENV=development DEBUG=api:* babel-node src/index.js",
    "test": "cross-env DEBUG=api:*,test:* NODE_ENV=test mocha --recursive --exit \"test/**/*.js\""
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "jsonpath": "1.0.0",
    "mocha": "5.0.0",
    "nodemon": "1.18.4",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "cross-env": "5.2.0",
    "debug": "3.1.0",
    "lodash": "4.17.10",
    "mongo-registry": "1.0.15",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "unit-manip": "1.0.7"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": []
      }
    }
  }
}
